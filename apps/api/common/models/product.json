{
  "name": "Product",
  "description": "Content model for managing Products in a Domain",
  "base": "BaseModel",
  "idInjection": true,
  "forceId": false,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "id": {
      "type": "String",
      "required": true
    },
    "domainId": {
      "type": "String",
      "required": false
    },
    "fileId": {
      "type": "String"
    },
    "name": {
      "type": "String",
      "required": false
    },
    "description": {
      "type": "String",
      "required": false
    },
    "sku": {
      "type": "String",
      "required": false
    },
    "price": {
      "type": "Number",
      "required": false
    }
  },
  "validations": [],
  "relations": {
    "domain": {
      "type": "belongsTo",
      "model": "Domain"
    },
    "file": {
      "type": "belongsTo",
      "model": "StorageFile"
    }
  },
  "acls": [],
  "methods": {}
}
